{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", "react-hooks", "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-use-before-define": "off",
        "ignoreTemplateLiterals": 0,
        "no-unused-vars": "off",
        "no-shadow":"off",
        "comma-dangle": ["error", "never"]
    }
}